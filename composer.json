{
    "name": "goodappr/moonshine-menu-manager",
    "version": "1.0.0",
    "description": "Drag-and-drop menu manager for MoonShine 4.x: multi-zone layout (sidebar, topbar, bottom bar), visibility toggle, per-user configuration",
    "keywords": [
        "moonshine",
        "laravel",
        "menu",
        "admin",
        "sidebar",
        "topbar",
        "drag-drop",
        "menu-manager"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/GoodAppR/moonshine-menu-manager",
    "authors": [
        {
            "name": "Yurii Bulavkin",
            "email": "iubulavkin@yandex.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/GoodAppR/moonshine-menu-manager/issues",
        "source": "https://github.com/GoodAppR/moonshine-menu-manager"
    },
    "require": {
        "php": "^8.2",
        "moonshine/moonshine": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MoonShine\\CustomMenuManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonShine\\CustomMenuManager\\Providers\\MenuManagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
